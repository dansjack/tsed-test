{
  "name": "tsed-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "dev": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "typescript": "^4.3.4"

  },
  "dependencies": {
    "@tsed/ajv": "^6.59.0",
    "@tsed/common": "^6.59.0",
    "@tsed/core": "^6.59.0",
    "@tsed/di": "^6.59.0",
    "@tsed/exceptions": "^6.59.0",
    "@tsed/json-mapper": "^6.59.0",
    "@tsed/platform-express": "^6.59.0",
    "@tsed/schema": "^6.59.0",
    "@tsed/swagger": "^6.59.0",
    "ajv": "^8.6.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.3.0",
    "@tsed/cli-plugin-mocha": "3.3.0",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^8.2.2",
    "@types/multer": "^1.4.6",
    "@types/node": "^15.12.4",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  }
}
